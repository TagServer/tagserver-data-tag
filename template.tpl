___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const sendPixel = require("sendPixel");
const getTimestampMillis = require("getTimestampMillis");
const encodeUriComponent = require("encodeUriComponent");
const Math = require('Math');
const JSON = require("JSON");
const getUrl = require("getUrl");
const getCookieValues = require("getCookieValues");
const setCookie = require("setCookie");
const generateRandom = require("generateRandom");

// const logToConsole = require("logToConsole");
//logToConsole(data);

// Set external id:
if (!getCookieValues("_tagserver_extid")[0]) {
  const extid = "tsx.1." + getTimestampMillis() + "." + generateRandom(0, 1000000000);
  setCookie(
    "_tagserver_extid",
    extid,
    {"secure": true, "max-age": 90 * 24 * 60 * 60} // 90 days, same as FB.
  );
}

const extid = getCookieValues("_tagserver_extid")[0];

// Build user data field:
const userData = {
  external_id: [extid],
};

if (data.userData) {
   for (let i = 0; i < data.userData.length; i++) {
     const item = data.userData[i];
     // TODO: yuck! hacky
     if (item.key != "external_id") {
       userData[item.key] = item.value;
     } else {
       userData[item.key].push(item.value);
     }
   }
}

// Build custom data field:
const customData = {};

if (data.customData) {
  for (let i = 0; i < data.customData.length; i++) {
     const item = data.customData[i];
     customData[item.key] = item.value;
   }
}

// Build payload:
const payload = {
  data: [{
    event_name: data.eventName,
    event_time: Math.round(getTimestampMillis() / 1000.0),
    event_id: data.eventId,
    action_source: "website",
    event_source_url: getUrl(),
    opt_out: data.optOut,
    user_data: userData,
    custom_data: customData,
    data_processing_options: [],
  }],
  test_event_code: data.testCode
};

// Send payload:
const url = data.tagserverUrl + "/events?pixel_id=" + data.pixelId + "&payload=" +
    encodeUriComponent(JSON.stringify(payload));
sendPixel(url, data.gtmOnSuccess, data.gtmOnFailure);


